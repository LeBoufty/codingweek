<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.telecomnancy.AdministrationController">
    <top>
        <fx:include source="header.fxml" />
    </top>
    <bottom>
        <fx:include source="footer.fxml" />
    </bottom>
   <center>
      <TableView fx:id="tableView" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="dateColumn" text="Date" />
            <TableColumn fx:id="idUtilisateurColumn" text="User ID" />
            <TableColumn fx:id="messageColumn" text="Message" />
         </columns>
      </TableView>
   </center>
</BorderPane>
